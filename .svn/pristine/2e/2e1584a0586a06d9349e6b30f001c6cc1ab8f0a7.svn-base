*{
   	margin: 0;
   	padding: 0;
   	font-family: "Microsoft Yahei";
}
input:focus, button:focus, textarea:focus{
   	outline: none;
}
a{
    cursor: pointer;
}
ul, li{
	list-style: none; 
}
body{
	padding:0;
	margin:0;
}
/* 表单样式 */
.panel{
	margin-bottom:0;
	background-color:#fff;
	-moz-box-shadow: 0 3px 0 rgba(12,12,12,0);
    -webkit-box-shadow: 0 3px 0 rgba(12,12,12,0);
    box-shadow: 0 3px 0 rgba(12,12,12,0);
}
/* 头部样式  */
.panel-heading{
	position: relative;
	background-color:#fff;
	border-radius:0;
}
.panel-heading-height{
	height:75px;
}
.panel-heading .headPosition{
	margin-top:0;
}
.headPosition a{
	color:#5bc0de;
}
.headPosition a:hover{
	color:#333;
	text-decoration: none;
}
.headPosition i{
	margin:0 10px;
}
/* 中间内容样式  */
.panel-body{
	border-radius:0;
}
.form-control:focus {
    border-color: #999;
    -moz-box-shadow: none;
    -webkit-box-shadow: none;
    box-shadow: none;
}
.row{
	margin:0;
}
.form-bordered .form-group:first-child {
    border-top: 1px solid #d3d7db;
}
.form-bordered>.form-group:first-child {
    border-top: none;
}
.form-bordered .form-borderTop{
    border-top: 1px solid rgba(0,0,0,0);
}
.place{
	padding-top:7px;
	color:#4a535e;
}
.timeCenter{
	padding-top:7px;
	color:#4a535e;
	text-align:center;
}
/* 图片上传样式 */
form{
	font-size:100%;
}
.zyupload form{
	border:1px solid #ccc;
	border-radius: 3px;
}
.upload_box{
	margin:0;
}
.upload_main{
	border:none;
}
.status_bar{
	border-top:none;
}
.upload_preview{
	border-bottom:none;
	min-height: 170px;
}
.add_upload, .upload_submit, .upload_inf, .imgPreview, .popup{
	display:none;
}
/* 表单查看时照片样式 */
.photoUl{
	border:1px solid #ccc;
	border-radius: 3px;
	min-height:170px;
	margin-bottom:0;
	overflow:hidden;
}
.viewingBg{
	background-color:#eee;
}
.photoUl li{
	margin:1em;
	float:left;
	border:1px solid #ccc;
	padding:5px;
	background: url("images/bg.png") no-repeat center 0;
	cursor: pointer;
}
.photoUl li span{
    display: table-cell;
    text-align: center;
    vertical-align: middle;
    width:130px;
    height:130px;
}
.photoUl li span div{
	width:130px;
    max-height:130px;
    margin:auto;
    overflow:hidden;
}
.photoUl li span div img{
	width:100%;
}
/* 待受理样式 */
textarea{
	resize: none;
}
/* 待评审样式 */
.video{
	margin-top:10px;
	min-height:100px;
}
.report{
	padding-top:7px;
	cursor: pointer;
	color:#428bca;
	display: inline-block;
}
.report:hover{
	color:#333;
}
/* 摄像头样式 */
.cameraHeight{
	height:322px;
}
.plugin{
	margin:0 auto;
}
.pluginHide{
	position: fixed;
	right:-99999px;
}
.form-control-select{
	height:39px;
}
.stopRecord{
	display:none;
}
/* 已鉴定样式 */
.hideTime{
	display:none;
}
/* 流程中不同状态显示不同模块 */
.module1, .module2, .module3, .module4, .module5{
	display:none;
}
/* 验证class */
.no{
	border:1px solid #f00 !important;
}
/* 与会人员选择样式 */
.participants{
	padding:0 5px;
	min-height:40px;
	margin-bottom:0;
	overflow: hidden;
}
.participants li{
	float:left;
	margin:5px;
	padding:5px 10px;
	background-color:#1caf9a;
	color:#fff;
	border-radius: 3px;
}
.participants .showBtn{
	background-color:#e6e6e6;
	color:#333;
	cursor: pointer;
}
.participants .fa{
	margin-left:10px;
	cursor: pointer;
	opacity: 0.5;
}
.participants .fa:hover{
	opacity: 1;
}
.personnelSelection{
	border:1px solid #ccc;
	border-radius: 3px;
	position: relative;
	display:none;
	margin-top:10px;
}
.retractBtn{
	position: absolute;
	top:5px;
	right:5px;
	width:30px;
	height:30px;
	text-align: center;
	line-height:30px;
	cursor: pointer;
	font-size:20px;
}
.retractBtn:hover{
	background-color:#ccc;
	border-radius: 3px;
}
.form-group-noBorder{
	border-top:none !important;
}
.personnelHeight{
	padding:5px;
	height:130px;
	border:1px solid #ccc;
	border-radius: 3px;
}
.personnelHeight span{
	margin:5px;
	padding:5px 10px;
	display:inline-block;
	width:10em;
	cursor: pointer;
}
.personnelHeight span:hover{
	border-radius: 3px;
	background-color:#1caf9a;
	color:#fff;
}
/* 表单最下面的按钮样式 */
.btnCenter{
	text-align: center;
}
.btnWidth{
	padding-left: 40px;
	padding-right: 40px;
	height:40px;
}
.notnull{
	border:1px solid #a94442;
}
.errors{
	color: #E07470;
	font-weight: bolder;
	font-size: 20px;
	position: absolute;
	top: 5px;
	margin-left: -15px;
}
.gdzhuangtai{
	display:none;
}

.chosen-container-single .chosen-single{
	margin-top:0;
	height:40px;
	line-height:30px;
	border-radius: 3px;
}
.chosen-container-single .chosen-single div{
	width:30px;
}
.chosen-container-single .chosen-single div b {
    background: url('../images/chosen-sprite.png') no-repeat 10px 8px;
}
.chosen-container-active.chosen-with-drop .chosen-single div b {
    background-position: -10px 13px;
}

/* 大屏展现样式  */
.timeWidth80{
	width:80%;
	float:left;
}
.timeSpanWidth{
	float:left;
	padding-left:10px;
	line-height:40px;
}
/* 设置屏数表单样式 */
.operationBtn .glyphicon{
	font-size: 20px;
    line-height: 40px;
    color: #ccc;
    cursor: pointer;
    padding:0 5px;
}
.operationBtn .glyphicon-remove-circle:hover{
	color:#900;
}
.operationBtn .glyphicon-circle-arrow-down:hover{
	color:#f0ad4e;
}
.addScreen{
	height:40px;
	padding:0 30px;
}
.screenLayout>div{
	width:100%;
	height:100%;
	display:none;
}
.screenLayout>div:first-child{
	display:block;
}
.height100{
	height:100%;
	border:1px solid #ccc;
	padding:10px 0;
}
.height50{
	height:50%;
	border:1px solid #ccc;
	padding:10px 0;
}
.noBorder{
	border:none;
}
.noPadding{
	padding:0;
}
.operationBtn, .displayTime{
	display:none;
}
.square{
	position: relative;
}
.square p{
	padding-left:10px;
	line-height:30px;
}
.square .setBtn{
	font-size:20px;
	position: absolute;
	top:20px;
	right:20px;
	cursor: pointer;
	color:#ccc;
}
.square .setBtn:hover{
	color:#333;
}
.screenNums .screenLayoutDiv{
	border-top:none;
}
/* logo设置样式  */
.photo{
	width:100%;
	height:92px;
	border:1px solid #ccc;
	border-radius:3px;
	text-align:center;
	padding:10px;
}
.photo img{
	width:auto;
	height:100%;
}
.inputFileBtn{
	width:92px;
	height:92px;
	border:1px solid #ccc;
	border-radius:3px;
	cursor:pointer;
	text-align:center;
}
.inputFileBtn .glyphicon{
	font-size:50px;
	color:#fddc63;
	line-height:90px;
}
.inputFile{
	display:none !important;
}
/* 多选框样式  */
.checkBox label{
	width:30%;
	display:inline-block;
	padding-left:20px;
	cursor: pointer;
}
/* 年月日时间选择样式  */
.monthTime, .dayTime{
	display:none;
}
.checkboxTIshi{
	padding-top:7px;
	color:#f00;
}
/* 抓图样式 */
.screenshot{
	margin-top:10px !important;
}
.imgPadding{
	padding:0 10px 10px 0;
	cursor: pointer;
}
/* 隐藏样式 */
.hidden{
	display:none;
}